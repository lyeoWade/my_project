RewriteEngine on
RewriteRule index.html$ index.php
RewriteRule online.html$ online.php
RewriteRule video.html$ video.php
RewriteRule index_(.*)$ index.php?page=$1
RewriteRule Injuries.html$ Injuries.php
RewriteRule news.html$ news.php
RewriteRule nba.html$ nba.php
RewriteRule login$ login.php
RewriteRule keyrank.html$ keyrank.php
RewriteRule weekrank.html$ weekrank.php
RewriteRule monthrank.html$ monthrank.php
RewriteRule quarterrank.html$ quarterrank.php
RewriteRule register.html$ register.php
RewriteRule contact.html$ contact.php
RewriteRule house$ house.php
RewriteRule my_article.html$ my_article.php
RewriteRule my_article.html?id=(.*)&page=(.*)$ my_article.php?id=$1&page=&2
RewriteRule my_guess(.*).html$ my_guess.php?id=$1
RewriteRule my_history_guess(.*).html$ my_guess_history.php?id=$1
RewriteRule guessList.html$ guess.php
RewriteRule my_collection.html$ my_collection.php
RewriteRule my_data.html$ my_data.php
RewriteRule push_article$ push_article.php
RewriteRule push_article_(.*)$ push_article.php?id=$1
RewriteRule NBAfirst.html$ first.php
RewriteRule content_(.*).html$ content.php?id=$1
RewriteRule videoDetail_(.*).html$ videoDetail.php?id=$1
RewriteRule onlineDetail.html$ onlineDetail.php
RewriteRule onlineDetail.html\?id\=(.*)$ onlineDetail.php?id=$1